                                <tr>
                                    <th>#</th>
                                    <th>User</th>
                                    <th>Media</th>
                                    <th>Device</th>
                                    <th>Time</th>
                                    <th>Stop</th>
                                </tr>
                                </thead>
                                <tbody>
                                {% for row_data in server_streams %}
                                <tr>
                                    <td>{{ loop.index + (page - 1) * per_page }}</td>
                                    <td>{{ row_data[0] }}</td>
                                    <td>{{ row_data[1] }}</td>
                                    <td>{{ row_data[2] }}</td>
                                    <td>{{ row_data[3] }}</td>
                                    <div class="pull-right action-buttons">
                                        <td>
                                            <p data-placement="top" data-toggle="tooltip"
                                               title="Delete">
                                                <button class="btn btn-danger btn-xs"
                                                <a data-id={{ row_data[4] }}
                                                   onclick="ConfirmLibraryDelete(this)"><img class="b-lazy" src="/static/image/16/rubbish-bin.png" height="20" width="20"></a>
                                        </td>
                                    </div>
                                </tr>
                                {% endfor %}
                                </tbody>
                            </table></div>
